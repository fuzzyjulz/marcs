.container
 .row
  .col-xs-12
   %h2 Log in

   = form_for(resource, as: resource_name, url: user_session_path()) do |f| 
    .field
     = f.label :fai
     %br
     = f.text_field :fai, autofocus: true
   
    .field
     = f.label :password
     %br
     = f.text_field :password, autocomplete: "off"
   
    -if devise_mapping.rememberable?
     .field
      = f.check_box :remember_me
      = f.label :remember_me
   
    .actions
     = f.submit "Log in"
   
   
   = render "users/shared/links"
